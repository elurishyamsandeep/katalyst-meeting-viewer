{
  "name": "katalyst-meeting-viewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:mcp": "concurrently \"npx @cocal/google-calendar-mcp start\" \"next dev --turbopack\"",
    "mcp:auth": "npx @cocal/google-calendar-mcp auth",
    "mcp:start": "npx @cocal/google-calendar-mcp start",
    "mcp:version": "npx @cocal/google-calendar-mcp version",
    "build": "next build",
    "start": "node scripts/setup-credentials.js && npm run dev:mcp",
    "start:prod": "node scripts/setup-credentials.js && npm run dev:mcp",
    "postinstall": "node scripts/setup-credentials.js || true",
    "lint": "next lint"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "google-auth-library": "^10.2.1",
    "googleapis": "^155.0.0",
    "groq-sdk": "^0.30.0",
    "lucide-react": "^0.536.0",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
